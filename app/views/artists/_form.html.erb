<%= form_for [@artist, @artist.songs.new] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :length %>
  <%= f.number_field :length %>

  <%= f.label :year %>
  <%= f.select :year, (1500..1950).to_a, :include_blank => {:year => "Select year"} %>

  <a href="javascript:;" id="add">Create</a>
<% end %>
